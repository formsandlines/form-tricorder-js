  "keywords": [
    "uFORM iFORM",
    "FORM logic",
    "undetermined form",
    "eigenFORM",
    "WELTFORM",
    "uFORM",
    "FORMWELT",
    "laws of form",
    "logic"
  ],
  "homepage": "https://tricorder.formform.dev",
  "author": {
    "name": "Peter Hofmann",
    "email": "peter.hofmann@formsandlines.eu",
    "url": "https://formsandlines.eu"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/formsandlines/form-tricorder"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "re:build": "rescript",
    "re:start": "rescript build -w"
  },
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "@vitejs/plugin-react": "1.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
